{
	"name": "expo-build-disk-cache",
	"description": "A plugin for Expo CLI that provides disk-based app build caching",
	"version": "0.4.2",
	"author": {
		"name": "WookieFPV (Lukas MÃ¼ller)",
		"url": "github.com/WookieFPV"
	},
	"license": "MIT",
	"main": "dist/index.js",
	"types": "dist/index.d.ts",
	"files": ["/dist"],
	"repository": {
		"type": "git",
		"url": "https://github.com/WookieFPV/expo-build-disk-cache"
	},
	"scripts": {
		"build": "tsup",
		"build:dev": "tsup --watch --clean false",
		"test": "bun test",
		"lint": "bunx biome check --fix",
		"lint:CI": "bunx biome check",
		"typecheck": "tsc",
		"api:codegen": "npx openapi-typescript src/cloudCache/openapi.json -o src/api/apiTypes.d.ts && bun run lint",
		"prepublishOnly": "bun i && bun run lint:CI && tsc && bun run build"
	},
	"dependencies": {
		"@expo/cli": "^0.24.13",
		"@expo/config": "^11.0.10",
		"cosmiconfig": "^9.0.0",
		"env-paths": "^2.2.1",
		"fs-extra": "^11.3.0",
		"openapi-fetch": "^0.13.7",
		"tar": "^7.4.3",
		"uuid": "^11.1.0",
		"xdg-basedir": "^5.1.0",
		"zod": "^4.0.0-beta.20250505T195954"
	},
	"devDependencies": {
		"@biomejs/biome": "1.9.4",
		"@total-typescript/ts-reset": "0.6.1",
		"@tsconfig/bun": "1.0.7",
		"@tsconfig/node18": "18.2.4",
		"@types/bun": "1.2.13",
		"@types/fs-extra": "11.0.4",
		"lefthook": "1.11.12",
		"openapi-typescript": "7.7.1",
		"tsup": "8.4.0",
		"typescript": "5.8.3"
	},
	"engines": {
		"node": ">=18.0.0"
	},
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"import": "./dist/index.js",
			"require": "./dist/index.js"

		},
		"./api": {
			"types": "./dist/api/index.d.ts",
			"import": "./dist/api/index.js",
			"require": "./dist/api/index.js"
		}
	}
}
